{
  "presets": [
    ["env", {
      "modules": false
    }],
    "stage-1",
    "react"
  ],
  "plugins": [
    "lodash",
    ["babel-plugin-transform-builtin-extend", {
      "globals": ["Error"]
    }],
    ["transform-runtime", {
      "useBuiltIns": true,
      "useESModules": true
    }]
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
